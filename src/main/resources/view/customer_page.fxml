<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1064.0" style="-fx-background-color: #032B08;"
            xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller.CustomerPageController">
    <children>
        <Pane layoutX="22.0" layoutY="25.0" prefHeight="675.0" prefWidth="139.0"
              style="-fx-background-color: #27F554; -fx-background-radius: 20;">
            <children>
                <Button layoutX="11.0" layoutY="34.0" mnemonicParsing="false" onAction="#dashaction" prefHeight="44.0"
                        prefWidth="117.0" style="-fx-background-color: #0ECF28; -fx-background-radius: 40;"
                        text="🏠 Dashboard">
                    <cursor>
                        <Cursor fx:constant="CROSSHAIR"/>
                    </cursor>
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <Button layoutX="11.0" layoutY="108.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="117.0"
                        style="-fx-background-color: #0C1C09; -fx-background-radius: 40;" text="🧍Customer"
                        textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <Button layoutX="11.0" layoutY="181.0" mnemonicParsing="false" onAction="#itemaction" prefHeight="44.0"
                        prefWidth="117.0" style="-fx-background-color: #0ECF28; -fx-background-radius: 40;"
                        text="🛍 Item">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <Button layoutX="11.0" layoutY="253.0" mnemonicParsing="false" onAction="#suppaction" prefHeight="44.0"
                        prefWidth="117.0" style="-fx-background-color: #0ECF28; -fx-background-radius: 40;"
                        text="🚛 Supplier">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <Button layoutX="11.0" layoutY="325.0" mnemonicParsing="false" onAction="#empaction" prefHeight="44.0"
                        prefWidth="117.0" style="-fx-background-color: #0ECF28; -fx-background-radius: 40;"
                        text="👮‍♂️ Employee">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                </Button>
                <Button layoutX="11.0" layoutY="617.0" mnemonicParsing="false" onAction="#logoutaction"
                        prefHeight="44.0" prefWidth="117.0"
                        style="-fx-background-color: #0ECF28; -fx-background-radius: 40;" text="🚪➡ Logout"/>
            </children>
        </Pane>
        <Label layoutX="191.0" layoutY="63.0" text="Customer" textFill="WHITE">
            <font>
                <Font name="Century Gothic Bold Italic" size="25.0"/>
            </font>
        </Label>
        <Pane layoutX="191.0" layoutY="110.0" prefHeight="591.0" prefWidth="848.0"
              style="-fx-background-color: #1F5414; -fx-background-radius: 20;">
            <children>
                <TableView fx:id="custtable" layoutX="21.0" layoutY="24.0" prefHeight="178.0" prefWidth="810.0"
                           style="-fx-background-color: #EEFCE3;">
                    <columns>
                        <TableColumn fx:id="custid" prefWidth="53.0" text="ID"/>
                        <TableColumn fx:id="custtitle" prefWidth="44.0" text="TITLE"/>
                        <TableColumn fx:id="custname" prefWidth="143.0" text="NAME"/>
                        <TableColumn fx:id="custdob" prefWidth="96.0" text="D.O.B"/>
                        <TableColumn fx:id="custsalary" prefWidth="103.0" text="SALARY"/>
                        <TableColumn fx:id="custaddress" prefWidth="94.0" text="ADDRESS"/>
                        <TableColumn fx:id="custcity" prefWidth="95.0" text="CITY"/>
                        <TableColumn fx:id="custprovince" prefWidth="100.0" text="PROVINCE"/>
                        <TableColumn fx:id="custpostcode" prefWidth="81.0" text="POST.CODE"/>
                    </columns>
                </TableView>
                <Button layoutX="90.0" layoutY="225.0" mnemonicParsing="false" onAction="#addaction" prefHeight="28.0"
                        prefWidth="80.0" style="-fx-background-color: #EC9141; -fx-background-radius: 20;" text="ADD">
                    <font>
                        <Font name="Bodoni MT Bold" size="15.0"/>
                    </font>
                </Button>
                <Button layoutX="279.0" layoutY="225.0" mnemonicParsing="false" onAction="#viewaction" prefHeight="28.0"
                        prefWidth="80.0" style="-fx-background-color: #17E88D; -fx-background-radius: 20;" text="VIEW">
                    <font>
                        <Font name="Bodoni MT Bold" size="15.0"/>
                    </font>
                </Button>
                <Button layoutX="457.0" layoutY="225.0" mnemonicParsing="false" onAction="#updateaction"
                        prefHeight="28.0" prefWidth="80.0"
                        style="-fx-background-color: #E817C2; -fx-background-radius: 20;" text="UPDATE">
                    <font>
                        <Font name="Bodoni MT Bold" size="15.0"/>
                    </font>
                </Button>
                <Button layoutX="645.0" layoutY="225.0" mnemonicParsing="false" onAction="#deleteaction"
                        prefHeight="28.0" prefWidth="80.0"
                        style="-fx-background-color: #E81717; -fx-background-radius: 20;" text="DELETE">
                    <font>
                        <Font name="Bodoni MT Bold" size="15.0"/>
                    </font>
                </Button>
                <TextField fx:id="txtid" layoutX="56.0" layoutY="308.0" prefHeight="25.0" prefWidth="207.0"
                           promptText="ID" style="-fx-background-color: #BBFCCA; -fx-background-radius: 5;"/>
                <TextField fx:id="txttitle" layoutX="57.0" layoutY="362.0" prefHeight="25.0" prefWidth="67.0"
                           promptText="Title" style="-fx-background-color: #BBFCCA; -fx-background-radius: 5;"/>
                <TextField fx:id="txtname" layoutX="160.0" layoutY="362.0" prefHeight="25.0" prefWidth="207.0"
                           promptText="Name" style="-fx-background-color: #BBFCCA; -fx-background-radius: 5;"/>
                <DatePicker fx:id="txtdate" layoutX="56.0" layoutY="410.0" prefHeight="25.0" prefWidth="180.0"
                            style="-fx-background-color: #BBFCCA; -fx-background-radius: 5;"/>
                <TextField fx:id="txtaddress" layoutX="56.0" layoutY="515.0" prefHeight="25.0" prefWidth="322.0"
                           promptText="Address" style="-fx-background-color: #BBFCCA; -fx-background-radius: 5;"/>
                <TextField fx:id="txtcity" layoutX="410.0" layoutY="515.0" prefHeight="25.0" prefWidth="117.0"
                           promptText="City" style="-fx-background-color: #BBFCCA; -fx-background-radius: 5;"/>
                <TextField fx:id="txtprovince" layoutX="549.0" layoutY="515.0" prefHeight="25.0" prefWidth="139.0"
                           promptText="Province" style="-fx-background-color: #BBFCCA; -fx-background-radius: 5;"/>
                <TextField fx:id="txtpost" layoutX="709.0" layoutY="515.0" prefHeight="25.0" prefWidth="117.0"
                           promptText="Postal Code" style="-fx-background-color: #BBFCCA; -fx-background-radius: 5;"/>
                <TextField fx:id="txtsalary" layoutX="56.0" layoutY="464.0" prefHeight="25.0" prefWidth="180.0"
                           promptText="Salary" style="-fx-background-color: #BBFCCA; -fx-background-radius: 5;"/>
            </children>
        </Pane>
        <Button layoutX="994.0" layoutY="53.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="45.0"
                style="-fx-background-color: #EF9A1A; -fx-background-radius: 100; -fx-font-size: 19;" text="🚹">
            <cursor>
                <Cursor fx:constant="CROSSHAIR"/>
            </cursor>
        </Button>
    </children>
</AnchorPane>
